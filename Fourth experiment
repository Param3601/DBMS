CREATE TABLE employee1 (  
    employee_id INT PRIMARY KEY,  
    employee_name VARCHAR(100),  
    emp_age INT,  
    emp_salary DECIMAL(10, 2)  
)

INSERT INTO EMPLOYEE1 VALUES (1, 'John Doe', 25, 50000)

