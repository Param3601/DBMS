SELECT COUNT(*) FROM employee1

SELECT MAX(emp_age) FROM employee1

SELECT MIN(emp_salary) FROM employee1

SELECT SUM(emp_age) FROM employee1 

SELECT AVG(emp_salary) FROM employee1

