CREATE TABLE customer (  
    Cust_id INT PRIMARY KEY,      
    Cust_name VARCHAR(50) NOT NULL,    
    Cust_email VARCHAR(50) NOT NULL,  
    Cust_phone VARCHAR(15) NOT NULL,  
    Cust_address VARCHAR(100)  
)

